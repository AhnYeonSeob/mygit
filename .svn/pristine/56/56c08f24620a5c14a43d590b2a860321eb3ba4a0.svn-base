package kr.or.ddit.yguniv.vo;

import java.io.Serializable;
import java.util.List;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.NotNull;
import javax.validation.constraints.Size;

import org.springframework.web.multipart.MultipartFile;

import kr.or.ddit.yguniv.validate.InsertGroup;
import lombok.Data;
import lombok.EqualsAndHashCode;

/**
 * 공결인정서접수
 */

@Data
@EqualsAndHashCode(of = {"stuId", "lectNo", "lectOrder", "weekCd"})
public class AbsenceCertificateReceiptVO implements Serializable {

	@NotBlank
	@Size(min = 10, max = 10)
	private String stuId; // 학번
	
	@NotBlank
	@Size(max = 4)
	private String weekCd; // 강의번호
	
	@NotBlank
	@Size(min = 2, max = 2)
	private Long lectOrder; // 강의차수
	
	@NotBlank
	@Size(min = 10, max = 10)
	private String lectNo; // 주차코드
	
	@NotBlank
	private String absenceResn; // 공결사유
		
	@NotBlank
	private Long atchFileId; // 파일그룹번호
	
	@NotNull(groups = InsertGroup.class)
	private MultipartFile file; // 공결 인증 제출 서류
	
	// select용 데이터
	private String id; // 학생 아이디
	private String nm; // 학생 이름
	private String lectNm; // 강의 차수명 
	private String sectNm; // 강의별 주차
	private String sectIdnty; // 이수 인정시간
	
	private String cocoCd; // 공통코드 코드
	private String parCocoCd; // 공통코드 상세코드
	private String cocoStts; // 공통코드 상태
	
	private AttendanceVO attendanceVO;
	private AttendeeVO attendeeVO;
	private StudentVO studentVO;
	private PersonVO personVO;
	private OrderLectureDataVO orderLectureDataVO;
	private LectureWeekVO lectureWeekVO;
	private LectureVO lectureVO;
	
	private CommonCodeVO commonCodeVO;
	private List<AtchFileDetailVO> atchFileDetailList;
}
