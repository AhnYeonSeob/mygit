package kr.or.ddit.yguniv.vo;

import java.io.Serializable;

import javax.validation.constraints.NotBlank;
import javax.validation.constraints.Size;

import lombok.Data;
import lombok.EqualsAndHashCode;

@Data
@EqualsAndHashCode(of = "teamCd")
public class ProjectTeamVO implements Serializable{
	@Size(max = 4)
	private String cocoCd;
	@NotBlank
	@Size(max = 10)
	private String teamCd;
	@NotBlank
	@Size(max = 10)
	private String lectNo;
	@NotBlank
	@Size(max = 10)
	private String stuId;
	@NotBlank
	@Size(max = 20)
	private String teamRepre;
	@NotBlank
	@Size(max = 20)
	private String teamPurpo;
	@NotBlank
	@Size(max = 3000)
	private String teamNotes;
	
	private Long teamEvscore;
	@NotBlank
	@Size(max = 8)
	private String teamDt;
	@NotBlank
	@Size(max = 8)
	private String teamEt;
	private String teamProge;
	
}
