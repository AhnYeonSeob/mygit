<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<h1>이의 신청 리스트</h1>
<h1>교수와 교직원 등록된 리스트</h1>

<table class="table table-bordered">

	<thead>
		<tr>
			<th>강의번호</th>
			<th>강의명</th>
			<th>학번</th>
			<th>이름</th>
			<th>조회버튼</th>
		</tr>		
	</thead>
		<tbody>
			<c:if test="${not empty dissList }">
				<c:forEach items="${dissList }" var="diss">
				
				<tr>
					<td>
						${diss.lectVO.lectNo }						
					</td>
					<td> 
						${diss.lectVO.lectNm }						
					</td>
					<td>				
						${diss.personVO.id }						
					</td>
					<td>
						${diss.personVO.nm }						
					</td>
					<td>
						<button id="findBtn" onclick="findOne('${diss.personVO.id }','${diss.lectVO.lectNo }');"> 조회 </button>					
					</td>
				</tr>		
				</c:forEach>
			</c:if>
		</tbody>
</table>


<form id="selForm" action="">
<div id="selDiv">
</div>
<div id="textDiv">
	
</div>
</form>

<script  src="${pageContext.request.contextPath }/resources/js/dissent/dissentScript.js"></script>

