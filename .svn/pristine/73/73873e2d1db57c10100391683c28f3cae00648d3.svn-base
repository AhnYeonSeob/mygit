			<th>PSWD_FAILR_CO</th>
			<td><input type="number" name="pswdFailrCo" class="form-control"
				required value="${person.pswdFailrCo}" /><span class="text-danger">${errors.pswdFailrCo}</span></td>
		</tr>
		<tr>
			<th>PROFL_PHOTO</th>
			<td><input type="text" name="proflPhoto" class="form-control"
				value="${person.proflPhoto}" /><span class="text-danger">${errors.proflPhoto}</span></td>
		</tr>
	</table>
</form:form>








