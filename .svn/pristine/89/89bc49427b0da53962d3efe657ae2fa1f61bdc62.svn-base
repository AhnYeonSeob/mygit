package kr.or.ddit.yguniv.vo;

import java.io.Serializable;
import java.time.LocalDate;

import javax.validation.constraints.NotNull;
import javax.validation.constraints.Size;

import org.springframework.format.annotation.DateTimeFormat;
import org.springframework.format.annotation.DateTimeFormat.ISO;

import lombok.Data;
import lombok.EqualsAndHashCode;


@Data
@EqualsAndHashCode(of="gdtCd")
public class GraduationVO implements Serializable{
	@NotNull
    @Size(max = 20)
	private String gdtCd; //인증자료번호
	
	@NotNull
    @Size(max = 10)
	private String stuId; //학생번호 
	
	@NotNull
    @Size(max = 10)
	private String empId; //담당교직원번호
	
	@Size(min = 4, max = 4)
	private String cocoCd; //졸업인증자료처리상태
	
	
	
	@NotNull
    @Size(max = 10)
	private String gdtType; //인증서류종류
	
	@NotNull
    @Size(max = 30)
	private String gdtNm;  //인증서류명
	
	@NotNull
    @Size(max = 30)
	private String gdtInst; //발급담당기관
	
	@NotNull
	@DateTimeFormat(iso=ISO.DATE)
	private LocalDate gdtIssu; //서류발급일자
	
	private Long  gdtScore; //자료할당점수
	
	@NotNull
	@Size(min = 8, max = 8)
	private String gdtTerm; //인증마감기한
	
	@NotNull
	private Long  atchFileId; //파일그룹번호
	
	
	//첨부파일그룹
	//학생
	//교직원
}
