function showPersonDetail(personId) {
    $.ajax({
        url: '../yguniv/person/detail/' + personId, // 요청 URL
        type: 'GET',
        success: function(response) {
            $('#personDetailContent').html(response);
            $('#personDetailModal').modal('show');
        },
        error: function() {
            alert("사용자 정보를 불러오는 데 실패했습니다.");
        }
    });
}


