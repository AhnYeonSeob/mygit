<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.yguniv.lecture.dao.LectureInquiryMapper">
	<resultMap type="LectureInquiryBoardVO" id="lectureInquiryList" autoMapping="true">
		
	</resultMap>
	<select id="selectLectureInquiryBoardList" resultMap="lectureInquiryList">
		SELECT CIB_CD
			, STU_ID
			, CIB_TTL
			, CIB_DT
			, CIB_ANS_DT
		FROM LECTURE_INQUIRY_BOARD
		WHERE LECT_NO = #{lectNo}
	</select>
	<select id="selectLectureInquiryBoard"></select>
	<insert id="insertLectureInquiryBoard"></insert>
	<update id="updateLectureInquiryBoard"></update>
	<delete id="deleteLectureInquiryBoard"></delete>
	
	<update id="insertLectureInquiryBoardAnswer"></update>
	<update id="updateLectureInquiryBoardAnswer"></update>
	<update id="deleteLectureInquiryBoardAnswer"></update>
</mapper>